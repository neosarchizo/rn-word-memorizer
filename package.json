{
  "name": "WordMemorizer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint src --ext .js,.ts,.tsx --quiet",
    "start": "react-native start",
    "test": "jest",
    "pretty": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:css": "stylelint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint ./src --ext \".js,.jsx,.ts,.tsx\" --fix && stylelint \"./src/**/*.css\" --fix",
    "apk": "react-native run-android --mode=release",
    "apk-install": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "apk-uninstall": "adb uninstall com.wordmemorizer",
    "bundle": "cd android && ./gradlew bundleRelease",
    "ios-project": "open ios/WordMemorizer.xcworkspace",
    "prepare": "husky",
    "adb-keyevent": "adb shell input keyevent 7",
    "launch": "adb shell am start -n com.wordmemorizer/com.wordmemorizer.MainActivity",
    "stop": "adb shell am force-stop com.wordmemorizer",
    "relaunch": "yarn stop && yarn launch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@tsconfig/react-native": "^3.0.3",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.21",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/semver": "^7.5.8",
    "@types/styled-components": "^5.1.34",
    "@types/styled-components-react-native": "^5.2.5",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.2.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.4",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
